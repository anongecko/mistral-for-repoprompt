{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/v1/models",
            "dest": "/api/index.js"
        },
        {
            "src": "/v1/completions",
            "dest": "/api/index.js"
        },
        {
            "src": "/v1/chat/completions",
            "dest": "/api/index.js"
        },
        {
            "src": "/v1",
            "dest": "/api/index.js"
        },
        {
            "src": "/chat/completions",
            "dest": "/api/index.js"
        },
        {
            "src": "/chat",
            "dest": "/api/index.js"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.js"
        }
    ]
}
